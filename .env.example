# VC Council Environment Variables
# Copy this file to .env and fill in your actual values

# ========== LLM Provider ==========
# Choose one: OpenAI or Anthropic
OPENAI_API_KEY=sk-...
# ANTHROPIC_API_KEY=sk-ant-...

# ========== Metorial MCP Platform ==========
# Get your API key from https://metorial.com
METORIAL_API_KEY=met_...
METORIAL_BASE_URL=https://api.metorial.com/v1

# ========== MCP Deployment IDs ==========
# Deploy MCPs on Metorial and paste deployment IDs here
MCP_APIFY_ID=deploy_...
MCP_GITHUB_ID=deploy_...
MCP_HACKERNEWS_ID=deploy_...
MCP_GDRIVE_ID=deploy_...
MCP_GCALENDAR_ID=deploy_...

# ========== Backend Configuration ==========
BACKEND_HOST=0.0.0.0
BACKEND_PORT=8000

# ========== Frontend Configuration ==========
# These are used by Vite
VITE_API_URL=http://localhost:8000
VITE_WS_URL=ws://localhost:8000
